            "properties": {
              "action_timeout": 180,
              "continue_on_failure": false,
              "description": "Validates that the Destination Organizations array is not empty and contains at least one organization name or ID.",
              "display_name": "Validate Destination Organizations",
              "script": "import sys\nimport json\n\n# Get the destination organizations input\ndest_orgs_input = sys.argv[1]\n\n# Check if array is empty\nif dest_orgs_input == \"[]\":\n    isValid = False\n    errorMessage = \"At least one destination organization must be provided.\"\nelse:\n    try:\n        # Try to parse as JSON to validate format\n        orgs = json.loads(dest_orgs_input)\n        if len(orgs) == 0:\n            isValid = False\n            errorMessage = \"At least one destination organization must be provided.\"\n        else:\n            isValid = True\n            errorMessage = \"\"\n    except json.JSONDecodeError:\n        isValid = False\n        errorMessage = \"Invalid JSON format for destination organizations.\"",
              "script_arguments": [
                "$workflow.definition_workflow_02T1E2S3T4W5O6R7K8F9L0O1W2T3E4S5T6Z.input.variable_workflow_02QEDL2MUEU1T3c9TCNRl8SHZmFiWq6Acup$"
              ],
